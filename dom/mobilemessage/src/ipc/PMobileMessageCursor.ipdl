include "mozilla/dom/mobilemessage/Types.h";

include protocol PSms;
include SmsTypes;

namespace mozilla {
namespace dom {
namespace mobilemessage {

protocol PMobileMessageCursor
{
  manager PSms;

child:
  NotifyResult(MobileMessageCursorData aData);
  NotifyError(int32_t aError);
  NotifyDone();

parent:
  /**
   * Sent when the child no longer needs to use mobile message cursor.
   */
  __delete__();

  Continue();
};

} // namespace mobilemessage
} // namespace dom
} // namespace mozilla
